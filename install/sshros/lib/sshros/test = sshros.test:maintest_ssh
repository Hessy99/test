#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'sshros','console_scripts','test = sshros.test:maintest_ssh'
__requires__ = 'sshros'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('sshros', 'console_scripts', 'test = sshros.test:maintest_ssh')()
    )
